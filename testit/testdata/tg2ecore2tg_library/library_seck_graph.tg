TGraph 2;
Schema org.testschema.ExtendedLibrarySchema;
GraphClass ExtendedLibraryGraph { version: String };
Package administration;
RecordDomain Article ( authornames: List<String>, keywords: List<String>, title: String );
RecordDomain BusinessHours ( openText: String, specialClosed: List<Date> );
EnumDomain Category ( KK, KB, JB, SL, S );
RecordDomain Date ( day: Integer, month: Integer, year: Integer );
RecordDomain ID ( category: Category, number: Long, year: Integer );
EnumDomain MediaType ( VHS, DVD, CD, CASSETTE, BLUERAY );
VertexClass Book: Medium;
VertexClass Library { openingHours: BusinessHours };
VertexClass Magazin: Medium { articles: List<Article>,  dateOfIssue: Date };
abstract VertexClass Medium { bibID: ID,  title: String };
VertexClass NewMedia: Medium { type: MediaType };
EdgeClass HasAuthor from Book (0,*) role book to people.Person (1,1) role author;
EdgeClass HasDirector from Library (0,1) role library aggregation shared to people.Person (1,1) role director;
EdgeClass HasMedia from Library (0,1) role library to Medium (0,*) role media aggregation shared;
EdgeClass Loans from people.User (0,*) role loaner to Medium (0,*) role items { dateOfLoan: Date,  dateOfReturn: Date };
Package people;
VertexClass Person { name: String };
VertexClass User: Person { identityNumber: Long };
